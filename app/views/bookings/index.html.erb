<h1>Bookings of  <%= current_user["first_name"] %>  <%= current_user["last_name"] %> </h1>


<% @bookings.each do |booking| %>
  <p>Your reservation on<%= booking.flat.planet %></p>
  <p>Name of the flat<%= booking.flat.name %></p>
  <p>From  <%= booking.start_time %></p>
  <p>To <%= booking.end_time%></p>
<% end %>

